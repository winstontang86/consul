{{#app-view class="token edit" loading=isLoading}}
    {{#block-slot 'notification' as |status type|}}
      {{!partial 'dc/tokens/notifications'}}
    {{/block-slot}}
    {{#block-slot 'breadcrumbs'}}
        <ol>
            <li><a data-test-back href={{href-to 'dc.acls.tokens'}}>All Tokens</a></li>
        </ol>
    {{/block-slot}}
    {{#block-slot 'header'}}
        <h1>
{{#if item.Name }}
            {{item.Name}}
{{else}}
            New Token
{{/if}}
        </h1>
    {{/block-slot}}
    {{#block-slot 'content'}}
        {{ partial 'dc/acls/tokens/form'}}
    {{/block-slot}}
{{/app-view}}